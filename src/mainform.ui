/*****************************************************************/
/* затрём все иконки - работаем с индикатормом*/
#btnMain, #btnFiles, #btnUSB, #btnSettings, #btnDocuments
{
	qproperty-icon:none;
}
#btnMain::indicator, #btnFiles::indicator,#btnUSB::indicator, 
#btnSettings::indicator, #btnDocuments::indicator
{
	width:  32px;
    height: 32px;
}

/* Главная */
#btnMain::indicator {
	image:url(:/resources/laptop.png);
}
#btnMain::indicator:hover, 
#btnMain::indicator:checked {
	image:url(:/resources/laptop_white.png);
}
